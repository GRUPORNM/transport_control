<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="transportadoraDialog"
		title="{i18n>transportadora}"
		items="{oViewModel>/listaTransportadora}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">

		<StandardListItem			
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{oViewModel>name1}"
			description="{oViewModel>kunnr}" />
	</SelectDialog>
</core:FragmentDefinition>
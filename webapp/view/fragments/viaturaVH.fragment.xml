<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="viaturaDialog"
		title="{i18n>viatura}"
		items="{oViewModel>/listaViatura}"
		search=".onValueHelpSearch"
		confirm=".onValueHelpClose"
		cancel=".onValueHelpClose">

		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{oViewModel>eqktx}"
			description="{oViewModel>equnr}" />
	</SelectDialog>
</core:FragmentDefinition>